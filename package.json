{
  "scripts": {
    "lint": "eslint --cache ./src ./bin --ext .js",
    "lint:watch": "esw --cache ./src ./bin --ext .js -w --color",
    "babel": "rm -rf ./lib && babel src --out-dir lib",
    "babel:watch": "rm -rf ./lib && babel -w src --out-dir lib",
    "test": "yarn babel && istanbul cover _mocha -- -R spec test/**/*.js",
    "test-server": "nodemon ./lib/server.js",
    "start": "NODE_ENV=production node ./lib/server.js",
    "build": "yarn babel",
    "prod": "export NODE_ENV=production && yarn babel",
    "watch": "concurrently -c green \"yarn lint:watch\" \"yarn babel:watch\"",
    "heroku-postbuild": "yarn prod"
  },
  "name": "open-data",
  "version": "0.1.0",
  "description": "Open data",
  "author": {
    "name": "HsuTing",
    "email": "hsuting0106@gmail.com",
    "url": "http://hsuting.com"
  },
  "main": "./lib/index.js",
  "keywords": [
    "open data"
  ],
  "pre-commit": [
    "lint"
  ],
  "homepage": "https://github.com/HsuTing/open-data/",
  "repository": {
    "type": "git",
    "url": "get+https://github.com/HsuTing/open-data.git"
  },
  "bugs": {
    "url": "https://github.com/HsuTing/open-data/issues/"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.5.0",
    "eslint": "^4.4.1",
    "eslint-config-google": "^0.9.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "pre-commit": "^1.2.2",
    "should": "^11.2.1"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "graphql": "^0.10.5",
    "graphql-relay": "^0.5.2",
    "koa": "^2.3.0",
    "koa-better-router": "^2.1.1",
    "koa-body": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-graphql": "^0.7.1",
    "koa-helmet": "^3.2.0",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^3.0.0",
    "nodemon": "^1.11.0"
  }
}
