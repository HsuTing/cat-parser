{
  "engines": {
    "node": "8.1.2",
    "yarn": "0.24.6",
    "npm": "5.3.0"
  },
  "scripts": {
    "getData": "rm -rf ./lib/bin && babel src/bin --out-dir lib/bin && node ./lib/bin/get-data.js",
    "create-schema": "rm -rf ./lib/bin && babel src/bin --out-dir lib/bin && node ./lib/bin/create-schema.js",
    "test": "jest --silent",
    "test:watch": "yarn test -- --watchAll",
    "lint": "eslint --cache ./src --ext .js",
    "lint:watch": "esw --cache ./src --ext .js -w --color",
    "babel": "rm -rf ./lib && babel src --out-dir lib",
    "babel:watch": "rm -rf ./lib && babel -w src --out-dir lib",
    "test-server": "nodemon ./lib/server.js",
    "start": "cross-env NODE_ENV=production node ./lib/server.js",
    "build": "yarn babel",
    "watch": "concurrently -c green \"yarn lint:watch\" \"yarn babel:watch\"",
    "heroku-postbuild": "yarn build"
  },
  "name": "open-data",
  "version": "0.1.0",
  "description": "Open data",
  "author": {
    "name": "HsuTing",
    "email": "hsuting0106@gmail.com",
    "url": "http://hsuting.com"
  },
  "main": "./lib/index.js",
  "keywords": [
    "open data"
  ],
  "pre-commit": [
    "lint"
  ],
  "homepage": "https://github.com/HsuTing/open-data/",
  "repository": {
    "type": "git",
    "url": "get+https://github.com/HsuTing/open-data.git"
  },
  "bugs": {
    "url": "https://github.com/HsuTing/open-data/issues/"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.5.0",
    "eslint": "^4.6.1",
    "eslint-config-google": "^0.9.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "inquirer": "^3.2.3",
    "jest": "^21.0.2",
    "pre-commit": "^1.2.2",
    "validator": "^8.1.0",
    "xml2js": "^0.4.19"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.1.0",
    "cross-env": "^5.0.5",
    "firebase": "^4.3.1",
    "geolib": "^2.0.23",
    "graphql": "0.10.5",
    "graphql-relay": "^0.5.2",
    "koa": "^2.3.0",
    "koa-better-router": "^2.1.1",
    "koa-body": "^2.3.0",
