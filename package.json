    "koa-etag": "^3.0.0",
    "koa-graphql": "^0.7.3",
    "koa-helmet": "^3.2.0",
    "koa-morgan": "^1.0.1",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "moment": "^2.18.1",
    "node-fetch": "^1.7.3",
    "nodemon": "^1.12.0"
  }
